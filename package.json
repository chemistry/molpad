    },
    "license": "MIT",
    "private": true,
    "keywords": [
        "cheminformatics",
        "molecular editor",
        "molecule sketcher"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "devEngines": {
        "node": ">=10.0.0"
    },
    "repository": {
