        "react-dom": "17.0.1"
    },
    "devDependencies": {
        "@types/jest": "26.0.19",
        "awesome-typescript-loader": "5.2.1",
        "css-loader": "3.6.0",
        "file-loader": "6.0.0",
        "html-webpack-plugin": "3.2.0",
        "jest": "24.9.0",
        "mini-css-extract-plugin": "0.9.0",
