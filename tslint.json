{
  "extends": "tslint:recommended",
  "linterOptions": {
      "exclude": [
          "node_modules/**",
          "**/node_modules/**",
          "**/dist/**"
      ]
  },
  "rules": {
    "curly": true,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-var-requires": false,
    "max-line-length": [
      true,
      140
    ]
  },
  "jsRules": {
    "curly": true
  },
  "rulesDirectory": [
  ]
}
